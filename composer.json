{
    "name": "pentiminax/ux-editor",
    "type": "symfony-bundle",
    "description": "Editor.js integration for Symfony",
    "keywords": [
        "symfony-ux",
        "editorjs"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pentiminax\\UX\\Editor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pentiminax\\UX\\Editor\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tanguy LemariÃ©",
            "email": "tanguy.lemarie6@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/orm-pack": "dev-main",
        "symfony/stimulus-bundle": "^2.22",
        "symfony/twig-bundle": "^7.0"
    },
    "conflict": {
        "symfony/flex": "<1.13"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/maker-bundle": "^1.62.1"
    }
}
