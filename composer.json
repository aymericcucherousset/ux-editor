{
    "name": "pentiminax/ux-editor",
    "type": "symfony-bundle",
    "description": "Editor.js integration for Symfony",
    "keywords": [
        "symfony-ux",
        "editorjs"
    ],
    "homepage": "https://github.com/pentiminax/ux-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Tanguy LemariÃ©",
            "email": "tanguy.lemarie6@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pentiminax\\UX\\Editor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pentiminax\\UX\\Editor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/stimulus-bundle": "^2.22"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.12",
        "phpunit/phpunit": "^11.5.17",
        "symfony/framework-bundle": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/var-dumper": "^7.0"
    },
    "conflict": {
        "symfony/flex": "<1.13"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "minimum-stability": "dev"
}
